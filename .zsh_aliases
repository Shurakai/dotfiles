alias ls='ls --color -h'
alias l='ls -al'
alias df='df -h'
alias mkdir='mkdir -p -v'
alias rm='rm -i'
alias which='type -p'
alias du='du -h'
alias top='htop'
alias grep='/bin/grep -i --color=always'
export EDITOR=/usr/bin/vim
export CFLAGS=' -O2 -mtune=core2 -march=core2 -D_SMP'
export CONCURRENCY_LEVEL=4
