alias -g ls='ls --color -h'
alias -g l='ls -al'
alias -g df='df -h'
alias -g mkdir='mkdir -p -v'
alias -g rm='rm -i'
alias -g which='type -p'
alias -g du='du -h'
alias -g top='htop'
alias -g grep='/bin/grep -i --color=always'
export EDITOR=/usr/bin/vim
export CFLAGS=' -O2 -mtune=core2 -march=core2 -D_SMP'
export CONCURRENCY_LEVEL=4
